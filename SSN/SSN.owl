    <!-- http://www.semanticweb.org/utilizador/ontologies/2018/4/untitled-ontology-22#Frequency -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/utilizador/ontologies/2018/4/untitled-ontology-22#Frequency">
        <rdfs:label rdf:datatype="&xsd;string">Frequency</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">The smallest possible time between one observation and the next.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="&xsd;string">http://purl.oclc.org/NET/ssnx/ssn</rdfs:isDefinedBy>
