        <rdfs:label rdf:datatype="&xsd;string">Start Time</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;string">http://purl.oclc.org/NET/ssnx/ssn</rdfs:isDefinedBy>
        <rdfs:isDefinedBy rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#startTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/utilizador/ontologies/2018/4/untitled-ontology-22#System -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/utilizador/ontologies/2018/4/untitled-ontology-22#System">
        <rdfs:label rdf:datatype="&xsd;string">System</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">System is a unit of abstraction for pieces of infrastructure (and we largely care that they are) for sensing. A system has components, its subsystems, which are other systems.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="&xsd;string">http://purl.oclc.org/NET/ssnx/ssn</rdfs:isDefinedBy>
        <rdfs:isDefinedBy rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#System"/>
    </owl:DatatypeProperty>
</rdf:RDF>



